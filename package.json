    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "joi": "^17.2.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^8.1.1",
    "multer": "^1.4.2",
    "pg": "^7.18.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.4",
